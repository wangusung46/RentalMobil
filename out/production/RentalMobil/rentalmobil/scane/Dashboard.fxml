<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane blendMode="MULTIPLY" prefHeight="420.0" prefWidth="720.0" style="-fx-background-color: #6dd5ed;" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="rentalmobil.controller.DashboardController">
   <children>
      <VBox prefHeight="420.0" prefWidth="720.0" style="-fx-background-color: #CCCC99;">
         <children>
            <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="100.0" prefWidth="720.0" style="-fx-background-color: #D3C4BE;" text="Dashboard" textAlignment="RIGHT" textOverrun="CENTER_WORD_ELLIPSIS">
               <font>
                  <Font size="36.0" />
               </font>
            </Label>
            <HBox prefHeight="295.0" prefWidth="200.0">
               <children>
                  <Button fx:id="buttonAdd" mnemonicParsing="false" onAction="#performAdd" prefHeight="40.0" prefWidth="720.0" text="Tambah" textAlignment="CENTER" textFill="#030303">
            </Button>
                  <Button fx:id="buttonView" mnemonicParsing="false" onAction="#performView" prefHeight="40.0" prefWidth="720.0" text="View" textFill="#141313">
            </Button>
                  <Button fx:id="buttonDelete" mnemonicParsing="false" onAction="#performDelete" prefHeight="40.0" prefWidth="720.0" text="Hapus" textFill="#110f0f">
            </Button>
               </children>
            </HBox>
            <Button fx:id="buttonLogout" mnemonicParsing="false" onAction="#performBack" prefHeight="25.0" prefWidth="720.0" text="Logout" />
         </children>
      </VBox>
   </children>
</AnchorPane>
